{
  "name": "js-testing",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@parcel/babel-preset-env": "^2.8.3",
    "babel-jest": "^29.3.1",
    "editorconfig-checker": "^5.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.1",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4"
  },
  "scripts": {
    "serve": "parcel index.html",
    "lint:prettier": "prettier --check .",
    "lint:spell": "yaspeller .",
    "lint": "yarn lint:prettier && yarn lint:code",
    "lint:code": "eslint --ext .js,.ts,.md .",
    "prepare": "husky install",
    "test": "jest",
    "build": "parcel index.html",
    "deploy": "cd dist --archive --compress --delete . iliafilimonov1@dev:/var/www/github.com/iliafilimonov1/dev/"
  },
  "lint-staged": {
    "*.{js,json,html,css,md,yml,yaml,ts,tsx}": "prettier --write",
    "{*js,*.md}": "eslint --cache --fix"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
